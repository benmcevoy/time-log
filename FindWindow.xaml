<Window x:Class="TimeLog.FindWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Find" Width="300" Height="100" ResizeMode="NoResize" >
    <Window.InputBindings>
        
    </Window.InputBindings>
    <StackPanel Orientation="Vertical">
        <TextBox x:Name="FindText"  Margin="5"  />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
            <Button IsDefault="True" Click="Button_Click" Content="Find" Margin="5" />
            <Button IsCancel="True" Content="Cancel" Margin="5" />
        </StackPanel>
    </StackPanel>
</Window>
